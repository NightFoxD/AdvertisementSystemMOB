<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ASProjektMOB.Views.SettingsPage"
             NavigationPage.HasBackButton="False" NavigationPage.HasNavigationBar="False">
    <ContentPage.Content>
        <ScrollView Margin="10,30,10,20">
            <StackLayout Orientation="Vertical" >
                <Button x:Name="Btn_Settings" Grid.Row="2" Text="Ustawienia" Clicked="Btn_Settings_Clicked"/>
                <Button x:Name="Btn_LogOut" Grid.Row="3" Text="Wyloguj się" Clicked="Btn_LogOut_Clicked"/>
            </StackLayout>
        </ScrollView>
    </ContentPage.Content>
</ContentPage>